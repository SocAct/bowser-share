{
  "name": "@socact/bowser-share",
  "version": "1.0.0",
  "description": "Share Libs for bowser. such wechat",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/tape/bin/tape ./test.js",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/tape/bin/tape ./test.js && istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "jshint": "jshint -c .jshintrc --exclude-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SocAct/bowser-share.git"
  },
  "pre-commit": [
    "jshint",
    "coverage"
  ],
  "keywords": [
    "share"
  ],
  "author": "kinglion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SocAct/bowser-share/issues"
  },
  "homepage": "https://github.com/SocAct/bowser-share#readme",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.5",
    "pre-commit": "^1.2.2",
    "tape": "^4.9.0"
  }
}
